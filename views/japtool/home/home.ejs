<div class="vocabulary content clearfix">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <h2 class="text-center clearfix">Learning Your Self</h2>

                <p class="text-center desc-home">Hiển thị những bài học của thành viên</p>
            </div>
            <div class="vocabulary-item">
                <% _.each(listLessons, function (listLesson, index) { %>
                <div class="info">
                    <div class="col-sm-12">
                        <h3><a class="text-primary"><%= listLesson.bookMaster.name %></a></h3>
                    </div>

                    <div class="col-sm-2">
                        <p>Category: <%= listLesson.bookMaster.type %></p>
                    </div>
                    <div class="col-sm-2">
                        <p>Level: <%= listLesson.bookMaster.level %></p>
                    </div>
                    <div class="col-sm-2">
                        <p>Lessoned: 3 Lesson</p>
                    </div>
                    <div class="col-sm-2">
                        <p>Pass: 2 Lesson</p>
                    </div>
                    <div class="col-sm-2">
                        <p>Failed: 1 Lesson</p>
                    </div>
                    <div class="col-sm-2">
                        <p>Tổng có: <%= listLesson.bookMaster.lessonNum %></p>
                    </div>
                    <div class="info-desc clearfix">
                        <% _.each(listItems, function (listItem, keyItem) { %>
                        <% if(index == keyItem ){ %>
                        <% _.each(listItem, function (item) { %>
                        <div class="col-sm-2">
                            <div class="info-lesson label-home">
                                <a onclick="Learn('<%= listLesson.id %>')"><%= item.lesson %></a>
                            </div>
                        </div>
                        <% }) %>
                        <% } %>
                        <% }) %>
                    </div>
                </div>
                <% }) %>
                <% _.each(listLessonsSkips, function (listLessonSkip, index) { %>
                <div class="info-book-more">
                    <div class="col-sm-12">
                        <div class="info-book clearfix">
                            <div class="col-sm-4">
                                <h3><%= listLessonSkip.bookMaster.name %></h3>
                            </div>
                            <div class="col-sm-4">
                                7/10 Bài đã học
                            </div>
                            <div class="col-sm-4">
                                Pass 5/ 10 Bài
                            </div>

                            <div class="info-desc disactive clearfix">
                                <div class="col-sm-2">
                                    <div class="info-lesson label-danger"><a>Lesson 1</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-danger"><a>Lesson 2</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-success"><a>Lesson 3</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-warning"><a>Lesson 4</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-home"><a>Lesson 1</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-home"><a>Lesson 2</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-home"><a>Lesson 3</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-home"><a>Lesson 4</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-home"><a>Lesson 1</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-home"><a>Lesson 2</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-home"><a>Lesson 3</a></div>
                                </div>
                                <div class="col-sm-2">
                                    <div class="info-lesson label-home"><a>Lesson 4</a></div>
                                </div>
                            </div>
                            <div class="col-sm-12">
                                <a class="view-detail-lesson view-detail-lesson-active pull-right">View Detail</a>
                                <a class="view-detail-lesson view-detail-lesson-hide pull-right disactive">Hide
                                    Detail</a>
                            </div>
                        </div>
                    </div>

                </div>
                <% }) %>
                <div class="col-sm-offset-4 col-sm-4">
                    <div class="info-lesson label-primary loadmore"><a>Load View More</a></div>
                </div>
            </div>
        </div>
    </div>
</div>
<form action="/japtool/learning/edit" method="post" id="edit-learning-japtool"
      onsubmit="return validateEditLearning()">
    <div class="modal fade" id="formedit" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg" id="editform"></div>
    </div>
</form>