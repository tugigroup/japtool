<% _.each(listItems, function (listItemsL,index) { %>
<%  _.each(listItemsL, function (item) {%>
<div class="col-sm-12">
    <h3><a class="text-primary"><%= item.name %></a></h3>
</div>
<div class="col-sm-2">
    <p class="text-primary">Category: <%= item.type %></p>
</div>
<div class="col-sm-2">
    <p class="text-info">Level: <%= item.level %></p>
</div>
<div class="col-sm-2">
    <p class="text-warning">Warning: <span class="lesson-warning"></span> Lesson</p>
</div>
<div class="col-sm-2">
    <p class="text-success">Pass: <span class="lesson-pass"></span> Lesson</p>
</div>
<div class="col-sm-2">
    <p class="text-danger">Failed: <span class="lesson-danger"></span> Lesson</p>
</div>
<div class="col-sm-2">
    <p class="text-primary">Tổng có: <%= item.lessonNum %></p>
</div>
<div class="info-desc clearfix">
    <% _.each(item.bookDetails, function (itemLesson) { %>
    <div class="col-sm-2 col-xs-3 col-md-1">
        <div class="info-lesson-home label-home" id="<%= itemLesson.id %>"><a
                    href="<%= itemLesson.id %>"><%= itemLesson.lesson %></a></div>
    </div>
    <% }) %>
</div>

<% }) %>
<% }) %>
<script type="text/javascript">
    var lessonSum = 0;
    <% _.each(selfLesson, function (selfLessonL) { %>
        <% _.each(selfLessonL, function (itemSelfLessonL) { %>
            <% if(itemSelfLessonL.status){ %>
                <% if(itemSelfLessonL.status == 'passed'){ %>
                $("#<%= itemSelfLessonL.bookDetail %>").removeClass('label-home');
                $("#<%= itemSelfLessonL.bookDetail %>").addClass('label-success');
                <% }else{ %>
                    $("#<%= itemSelfLessonL.bookDetail %>").removeClass('label-home');
                    $("#<%= itemSelfLessonL.bookDetail %>").addClass('label-danger');
                <% } %>
            <% } else{ %>
                $("#<%= itemSelfLessonL.bookDetail %>").removeClass('label-home');
                $("#<%= itemSelfLessonL.bookDetail %>").addClass('label-warning');
            <%}%>
        <%})%>
    <%})%>

</script>