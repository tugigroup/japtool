<div class="container user-new">
    <div class="row">
        <h1>Register Account</h1>

        <div class=" clearfix">
            <div class="col-sm-8 row-no-padding-left">
                <p>If you already have an account with us, please login at the <a
                            href="/japtool/auth">login page</a>.</p>
            </div>
            <div class="col-sm-2 row-no-padding-right">
                <a class="btn btn-block btn-sm btn-social btn-facebook" href="/auth/facebook">
                    <i class="fa fa-facebook"></i> Sign in with Facebook
                </a>
            </div>
            <div class="col-sm-2 row-no-padding-right">
                <a class="btn btn-block btn-sm btn-social btn-google-plus" href="/auth/google">
                    <i class="fa fa-google-plus"></i> Sign in with Google
                </a>
            </div>
        </div>
        <% if(flash && flash.err) { %>
        <ul class="alert alert-success">
            <% Object.keys(flash.err).forEach(function(error) { %>
            <li><%- JSON.stringify(flash.err[error]) %></li>
            <% }) %>
        </ul>
        <% } %>
        <form action="/japtool/user/create" method="POST"
              enctype="multipart/form-data" class="form-horizontal form-register">
            <fieldset id="account">
                <legend>Your Personal Details</legend>
                <div class="form-group required">
                    <label class="col-sm-2 control-label" for="input-username">Username</label>

                    <div class="col-sm-10">
                        <input type="text" name="username" value="Happy Coding" placeholder="Username" id="input-username"
                               class="form-control">
                    </div>
                </div>
                <div class="form-group required">
                    <label class="col-sm-2 control-label" for="input-firstname">First Name</label>

                    <div class="col-sm-10">
                        <input type="text" name="firstname" value="Tuan" placeholder="First Name" id="input-firstname"
                               class="form-control">
                    </div>
                </div>
                <div class="form-group required">
                    <label class="col-sm-2 control-label" for="input-lastname">Last Name</label>

                    <div class="col-sm-10">
                        <input type="text" name="lastname" value="Nguyen Trong" placeholder="Last Name" id="input-lastname"
                               class="form-control">
                    </div>
                </div>
                <div class="form-group required">
                    <label class="col-sm-2 control-label" for="input-email">E-Mail</label>

                    <div class="col-sm-10">
                        <input type="email" name="email" value="tuannt22@gmail.com" placeholder="E-Mail" id="input-email"
                               class="form-control">
                    </div>
                </div>

            </fieldset>
            <fieldset>
                <legend>Your Password</legend>
                <div class="form-group required">
                    <label class="col-sm-2 control-label" for="input-password">Password</label>

                    <div class="col-sm-10">
                        <input type="password" name="password" value="123456" placeholder="Password" id="input-password"
                               class="form-control">
                    </div>
                </div>
                <div class="form-group required">
                    <label class="col-sm-2 control-label" for="input-confirm">Password Confirm</label>

                    <div class="col-sm-10">
                        <input type="password" name="confirmation" value="123456" placeholder="Password Confirm"
                               id="input-confirm" class="form-control">
                    </div>
                </div>
            </fieldset>
            <input type='hidden' name='_csrf' value='<%= _csrf %>'>

            <div class="buttons submit-user-new clearfix">
                <div class="pull-right">
                    <input type="submit" value="Continue" class="btn btn-primary">
                </div>
            </div>
        </form>
    </div>
</div>


