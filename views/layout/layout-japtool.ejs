<body>
<div class="header">
  <div class="header-top">
    <div class="container">
      <div class="row">
        <div class="account-right col-sm-7 col-sm-offset-5">
          <% if(session.authenticated){ %>
          <span>Welcome <a id="userIdInSession" userId="<%= session.User.id %>"
                           href="/japtool/user/show/<%= session.User.id %>"><%= session.User.username %></a></span><a
            href="/japtool/auth/destroy">Sign Out</a>
          <% } else {%>
          <a href="/japtool/auth">Login</a>
          <a href="/japtool/user/new">Register</a>
          <% } %>
        </div>
      </div>
    </div>
  </div>
  <div class="header-center clearfix">
    <div class="container">
      <div class="row">
        <div class="col-sm-3">
          <a class="logo"><img src="/images/logo.png"></a>
        </div>
        <div class="col-md-9">
          <div id='cssmenu' class="pull-right">
            <ul class="menu-page">
              <li><a href='/japtool/learning/home'><%= __('Home') %></a></li>
              <li><a href='/japtool/page/about'><%= __('About') %></a></li>

              <li class='active'><a href='/japtool/BookMaster/getLibrary'><%= __('Library') %></a>
              </li>
              <li><a href="/japtool/learning"><%= __('Learning') %></a></li>
              <li><a href="/japtool/page/guide"><%= __('Use Guide') %></a></li>
              <li><a href='/japtool/page/contact'><%= __('Contact') %></a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>

    <div class="stripe_color"></div>
    <div class="stripe_image"></div>
  </div>
</div>

